INFORMANT-HYPERV-PERF DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, OBJECT-IDENTITY, Gauge32
        FROM SNMPv2-SMI
    InstanceName, informant
        FROM WTCS;

informantHyperVPerf MODULE-IDENTITY
    LAST-UPDATED "201304240429Z"
    ORGANIZATION "Informant Systems, Inc."
    CONTACT-INFO
            "Garth Williams
             11135-23A Ave
             Edmonton, AB  T6J4W5  Canada
             Tel: +1 780 434 4113
             E-mail: garth.williams@wtcs.org"
    DESCRIPTION
            "The MIB module for SNMP Informant Hyper-V
             performance counter entries."
    REVISION      "201304240429Z"
    DESCRIPTION
            "Added additional entries which are available
             in Windows Server 2012."
    REVISION      "200912072108Z"
    DESCRIPTION
            "The initial revision of this MIB module."
    ::= { informant 25 }

hypervisor OBJECT-IDENTITY
    STATUS     current
    DESCRIPTION
	    "Information on the hypervisor."
    ::= { informantHyperVPerf 1 }

hvLogicalProcessors OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "The number of logical processors present in
	    the system."
    ::= { hypervisor 1 }

hvMonitoredNotifications OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "The number of monitored notifications
	    registered with the hypervisor."
    ::= { hypervisor 2 }

hvPartitions OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "The number of partitions (virtual machines)
	    present in the system."
    ::= { hypervisor 3 }

hvTotalPages OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "The number of bootstrap and deposited pages in
	    the hypervisor."
    ::= { hypervisor 4 }

hvVirtualProcessors OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "The number of virtual processors present in
	    the partition."
    ::= { hypervisor 5 }

hypervisorLogicalProcessorTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF HypervisorLogicalProcessorEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Information on logical processors."
    ::= { informantHyperVPerf 2 }

hypervisorLogicalProcessorEntry OBJECT-TYPE
    SYNTAX     HypervisorLogicalProcessorEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'Hyper-V Hypervisor Logical
	    Processor' object."
    INDEX      { hvlpInstance }
    ::= { hypervisorLogicalProcessorTable 1 }

HypervisorLogicalProcessorEntry ::= SEQUENCE {
    hvlpInstance InstanceName,
    hvlpPercentC1Time Gauge32,
    hvlpPercentC2Time Gauge32,
    hvlpPercentC3Time Gauge32,
    hvlpPercentGuestRunTime Gauge32,
    hvlpPercentHypervisorRunTime Gauge32,
    hvlpPercentIdleTime Gauge32,
    hvlpPercentTotalRunTime Gauge32,
    hvlpC1TransitionsPerSec Gauge32,
    hvlpC2TransitionsPerSec Gauge32,
    hvlpC3TransitionsPerSec Gauge32,
    hvlpContextSwitchesPerSec Gauge32,
    hvlpHardwareInterruptsPerSec Gauge32,
    hvlpInterProcessorIntrSentPerSec Gauge32,
    hvlpInterProcessorInterrupPerSec Gauge32,
    hvlpMonitorTransitionCost Gauge32,
    hvlpSchedulerInterruptsPerSec Gauge32,
    hvlpTimerInterruptsPerSec Gauge32,
    hvlpTotalInterruptsPerSec Gauge32,
    hvlpPercentOfMaxFrequency Gauge32,
    hvlpFrequency Gauge32,
    hvlpParkingStatus Gauge32,
    hvlpProcessorStateFlags Gauge32,
    hvlpRootVpIndex Gauge32
}

hvlpInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { hypervisorLogicalProcessorEntry 1 }

hvlpPercentC1Time OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "% C1 Time is the percentage of time the
	    processor spends in the C1 low-power idle
	    state. % C1 Time is a subset of the total
	    processor idle time."
    ::= { hypervisorLogicalProcessorEntry 2 }

hvlpPercentC2Time OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "% C2 Time is the percentage of time the
	    processor spends in the C2 low-power idle
	    state. % C2 Time is a subset of the total
	    processor idle time."
    ::= { hypervisorLogicalProcessorEntry 3 }

hvlpPercentC3Time OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "% C3 Time is the percentage of time the
	    processor spends in the C3 low-power idle
	    state. % C3 Time is a subset of the total
	    processor idle time."
    ::= { hypervisorLogicalProcessorEntry 4 }

hvlpPercentGuestRunTime OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "The percentage of time spent by the virtual
	    processor in guest code."
    ::= { hypervisorLogicalProcessorEntry 5 }

hvlpPercentHypervisorRunTime OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
