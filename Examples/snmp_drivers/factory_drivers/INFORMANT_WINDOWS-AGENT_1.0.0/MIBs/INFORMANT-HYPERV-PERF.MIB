    hvrCompressionEfficiency Gauge32,
    hvrLastReplicationSize Gauge32,
    hvrNetworkBytesRecv Gauge32,
    hvrNetworkBytesSent Gauge32,
    hvrReplicationCount Gauge32,
    hvrReplicationLatency Gauge32,
    hvrResynchronizedBytes Gauge32
}

hvrInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { hvReplicaVMEntry 1 }

hvrAverageReplicationLatency OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the average
	    replication latency in seconds."
    ::= { hvReplicaVMEntry 2 }

hvrAverageReplicationSize OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the average
	    replication size in bytes."
    ::= { hvReplicaVMEntry 3 }

hvrCompressionEfficiency OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the compression
	    efficiency for the files that has been
	    transferred over the network."
    ::= { hvReplicaVMEntry 4 }

hvrLastReplicationSize OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the last replication
	    size in bytes."
    ::= { hvReplicaVMEntry 5 }

hvrNetworkBytesRecv OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the total bytes
	    received over the network for the virtual
	    machine since the virtual machine management
	    service was started."
    ::= { hvReplicaVMEntry 6 }

hvrNetworkBytesSent OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the total bytes sent
	    over the network for the virtual machine since
	    the virtual machine management service was
	    started."
    ::= { hvReplicaVMEntry 7 }

hvrReplicationCount OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the replication count
	    since the virtual machine management service
	    was started."
    ::= { hvReplicaVMEntry 8 }

hvrReplicationLatency OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the last replication
	    latency in seconds. It is the time taken for
	    the delta to be applied on the recovery since
	    it was snapped."
    ::= { hvReplicaVMEntry 9 }

hvrResynchronizedBytes OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the total bytes sent
	    and received over the network for the virtual
	    machine during the resynchronize operation
	    since the virtual machine management service
	    was started."
    ::= { hvReplicaVMEntry 10 }

hvVirtualIDEControlEmulatedTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF HyperVVirtualIDEControllerEmulatedEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance counters for a virtual machine's
	    IDE Controller."
    ::= { informantHyperVPerf 28 }

hvVirtualIDEControlEmulatedEntry OBJECT-TYPE
    SYNTAX     HyperVVirtualIDEControllerEmulatedEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'Hyper-V Virtual IDE Controller
	    (Emulated)' object."
    INDEX      { hvviceInstance }
    ::= { hvVirtualIDEControlEmulatedTable 1 }

HyperVVirtualIDEControllerEmulatedEntry ::= SEQUENCE {
    hvviceInstance InstanceName,
    hvviceReadBytesPerSec Gauge32,
    hvviceReadSectorsPerSec Gauge32,
    hvviceWriteBytesPerSec Gauge32,
    hvviceWrittenSectorsPerSec Gauge32
}

hvviceInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { hvVirtualIDEControlEmulatedEntry 1 }

hvviceReadBytesPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the total number of
	    bytes that have been read per second on this
	    virtual device."
    ::= { hvVirtualIDEControlEmulatedEntry 2 }

hvviceReadSectorsPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Read Sectors/sec is the number of sectors read
	    per second from the disks attached to the IDE
	    controller."
    ::= { hvVirtualIDEControlEmulatedEntry 3 }

hvviceWriteBytesPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the total number of
	    bytes that have been written per second on this
	    virtual device."
    ::= { hvVirtualIDEControlEmulatedEntry 4 }

hvviceWrittenSectorsPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Written Sectors/sec is the number of sectors
	    written per second to the disks attached to the
	    IDE controller."
    ::= { hvVirtualIDEControlEmulatedEntry 5 }

hvVirtualSwitchProcessorTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF HyperVVirtualSwitchProcessorEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "This counter set represents the statistics for
	    the Microsoft Hyper-V switch processor."
    ::= { informantHyperVPerf 29 }

hvVirtualSwitchProcessorEntry OBJECT-TYPE
    SYNTAX     HyperVVirtualSwitchProcessorEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
	    "Performance information for one particular
	    instance of 'Hyper-V Virtual Switch Processor'
	    object."
    INDEX      { hvvswInstance }
    ::= { hvVirtualSwitchProcessorTable 1 }

HyperVVirtualSwitchProcessorEntry ::= SEQUENCE {
    hvvswInstance InstanceName,
    hvvswNumberOfSendChannels Gauge32,
    hvvswNumberOfVMQs Gauge32
}

hvvswInstance OBJECT-TYPE
    SYNTAX     InstanceName
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Instance name."
    ::= { hvVirtualSwitchProcessorEntry 1 }

hvvswNumberOfSendChannels OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the number of send
	    interrupt channels targeting on the virtual
	    switch processor."
    ::= { hvVirtualSwitchProcessorEntry 2 }

hvvswNumberOfVMQs OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "This counter represents the number of VMQs
	    targeting on the virtual switch processor."
    ::= { hvVirtualSwitchProcessorEntry 3 }

siHyperVPerformanceVersion OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "Designates the version of this SNMP Informant
	    agent."
    ::= { informantHyperVPerf 999 }

