hvlpPercentTotalRunTime OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "The percentage of time spent by the virtual
	    processor in guest and hypervisor code."
    ::= { hypervisorLogicalProcessorEntry 8 }

hvlpC1TransitionsPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "C1 Transitions/sec is the rate that CPU enters
	    the C1 low-power idle state."
    ::= { hypervisorLogicalProcessorEntry 9 }

hvlpC2TransitionsPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
	    "C2 Transitions/sec is the rate that CPU enters
	    the C2 low-power idle state."
    ::= { hypervisorLogicalProcessorEntry 10 }

hvlpC3TransitionsPerSec OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
